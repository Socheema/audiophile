{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020", "DOM"],
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "allowJs": false,
    "noEmit": true
  },
  "include": ["src", "convex", "types"],
  "exclude": ["node_modules", "dist", "build"]
}
